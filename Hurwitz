## The groups of type E6 #####################################################
##
## The code for E6(q) where q is odd is as follows. Again it can be modified by specifying q.

q:=3;;
Z:=PrimitiveElement(GF(q));;

I:=ScalarMatrix(GF(q),27,1);;
phia:=Matrix(GF(q),27,27,[<1,2,1>,<11,13,1>,<14,16,1>,<17,18,1>,<19,20,1>,<21,22,1>]);;
phib:=Matrix(GF(q),27,27,[<4,5,1>,<6,7,1>,<8,10,1>,<19,21,1>,<20,22,1>,<23,24,1>]);;
phic:=Matrix(GF(q),27,27,[<2,3,1>,<9,11,1>,<12,14,1>,<15,17,1>,<20,23,1>,<22,24,1>]);;
phid:=Matrix(GF(q),27,27,[<3,4,1>,<7,9,1>,<10,12,1>,<17,19,1>,<18,20,1>,<24,25,1>]);;
phie:=Matrix(GF(q),27,27,[<4,6,1>,<5,7,1>,<12,15,1>,<14,17,1>,<16,18,1>,<25,26,1>]);;
phif:=Matrix(GF(q),27,27,[<6,8,1>,<7,10,1>,<9,12,1>,<11,14,1>,<13,16,1>,<26,27,1>]);;

xa:=I+phia;xa:=GL(27,q)!xa;
xb:=I+phib;xb:=GL(27,q)!xb;
xc:=I+phic;xc:=GL(27,q)!xc;
xd:=I+phid;xd:=GL(27,q)!xd;
xe:=I+phie;xe:=GL(27,q)!xe;
xf:=I+phif;xf:=GL(27,q)!xf;

ha:=GL(27,q)!DiagonalMatrix(GF(q),27,[Z,Z^(-1),1,1,1,1,1,1,1,1,Z,1,Z^(-1),Z,1,Z^(-1),Z,Z^(-1),Z,Z^(-1),Z,Z^(-1),1,1,1,1,1]);;

n:=GL(27,q)!Matrix(GF(q),27,27,[<1,10,1>,<7,12,1>,<9,14,1>,<10,15,1>,<11,16,1>,<12,17,1>,<14,18,1>,
<15,21,1>,<17,22,1>,<18,9,1>,<19,24,1>,<20,11,1>,<21,27,1>,<22,19,1>,<23,13,1>,<24,20,1>,<25,23,1>,
